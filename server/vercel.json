{
"version": 2,
"builds": [
    { "src": "*.js", 
        "use": "@vercel/node" }],
"routes": [
        {
          "src": "/(.*)",
          "dest": "./index.js",
          "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
          "headers": {
            "Access-Control-Allow-Origin": "*"
          }
        }
      ],
      "headers": [
        {
          "source": "/(.*)",
          "headers": [
            { "key": "Access-Control-Allow-Origin", "value": "*" }
          ]
        },
        {
          "source": "/pantrypal-jet.vercel.app/(.*)",
          "headers": [
            { "key": "Access-Control-Allow-Origin", "value": "*" }
          ]
        } 
    ]

}